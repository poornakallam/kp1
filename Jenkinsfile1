pipeline {
   agent any 
   
 tools {
  maven "maven 3.8.3"
  }
  
 triggers {
   pollSCM('* * * * *')
   }
 stages {
   stage ('git') {
     steps {
       git 'https://github.com/poornakallam/maven-web-application.git'
       }
       }
    stage ('maven')
      steps {
       sh "mvn clean package"
       }
       }
       
   }
       
       
